<div class="row justify-content-md-center">
  <div class="col table-responsive-md">

  <a href="/contact-list/add" class="btn btn-primary btn-lg"><i class="fas fa-plus-circle"></i> Add New Contact</a>

    <table class="table table-dark">
      <thead>
        <tr>
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">Address</th>
          <th scope="col">Phone Number</th>
          <th scope="col">Email</th>
        </tr>
      </thead>
      <tbody>
             <% for(let index=0; index<contactList.length; index++) {%> 
            <tr>
                <th><%= contactList[index].firstname%> </th>
                <td><%= contactList[index].lastname%></td>
                <td><%= contactList[index].address%></td>
                <td><%= contactList[index].phonenumber%></td>
                <td><%= contactList[index].email%></td>
            </tr> 
            <%}%> 
      </tbody>

    </table>

  </div>
</div>